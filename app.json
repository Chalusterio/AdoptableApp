{
  "expo": {
    "name": "Adoptable",
    "slug": "AdoptableApp",
    "scheme": "com.adoptable.app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/adoptable-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/adoptable-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "INTERNET"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logo-adoptable.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.missel012.AdoptableApp"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-build-properties",
      "expo-secure-store"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "86c59580-98fa-4220-891f-007435e05c1c"
      }
    }
  }
}
